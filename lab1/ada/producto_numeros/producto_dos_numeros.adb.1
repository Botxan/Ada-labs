WITH Ada.Text_IO;
USE Ada.Text_IO;

PROCEDURE producto_dos_numeros IS
   num1: Integer;
   num2: Integer;
   producto: Integer := num1 * num2;
BEGIN
   put("Introduzca el primer numero: ");
   get(num1);

   new_line;

   put("Introduzca el segundo numero: ");

   new_line;
   mew_line;
   put("El resultado es: " & Integer'Image(producto));

END producto_dos_numeros;